DB_HOST=classicpress-db
DB_NAME=test
DB_USER=root
DB_PASSWORD=123
DB_PREFIX=wp_

WP_ENV=development
WP_HOME=https://test.com
WP_SITEURL="${WP_HOME}/wp"

WP_DEBUG=true
# WP_DEBUG_DISPLAY and WP_DEBUG_LOG perform no function unless WP_DEBUG is true
WP_DEBUG_DISPLAY=true
# log file in wp-content/debug.log
WP_DEBUG_LOG=true
# enable loading of non-minified, non-concatenated scripts and stylesheets
SCRIPT_DEBUG=true
# classicpress deprecations
WP_SHOW_DEPRECATIONS=false
# core, plugin, all
WP_DEVELOPMENT_MODE=false

# Generate your keys here: https://roots.io/salts.html
AUTH_KEY='generateme'
SECURE_AUTH_KEY='generateme'
LOGGED_IN_KEY='generateme'
NONCE_KEY='generateme'
AUTH_SALT='generateme'
SECURE_AUTH_SALT='generateme'
LOGGED_IN_SALT='generateme'
NONCE_SALT='generateme'
